alan = 3
sofia = 5
antonio = 6
print("Alan tiene: ", alan, "manzanas")
print("sofia tiene: ", sofia, "manzanas")
print("antonio tiene: ", sofia, "manzanas")
canasta = alan + sofia + antonio
print("El total de manzanas en la canasta es: ", canasta)
